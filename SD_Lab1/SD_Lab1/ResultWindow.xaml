<Window x:Class="SD_Lab1.ResultWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SD_Lab1"
        mc:Ignorable="d"
        Title="" Height="200" Width="400" Background="Black" Loaded="Window_Loaded">
    <StackPanel>


        <RichTextBox Name="Output_Result" Background="Black" Foreground="White" Margin="5">

        </RichTextBox>

        <Button Content="Save" Click="Save_Click" Background="Black" Foreground="White" Margin="5"/>

        <Button Content="Period" Click="Period_Click"  Background="Black" Foreground="White" Margin="5"/>

    </StackPanel>
</Window>
